#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/web-components/bin/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/web-components/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/web-components/bin/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/web-components/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules/@storybook/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@storybook+web-components@6.5.10_qofjau5ec5ae22w372j73bd22e/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/build.js" "$@"
else
  exec node  "$basedir/../../bin/build.js" "$@"
fi
