#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/dev-server/dist/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/dev-server/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/dev-server/dist/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/dev-server/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules/@web/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/@web+dev-server@0.1.32/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@web+dev-server@0.1.32/node_modules/@web/dev-server/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../@web+dev-server@0.1.32/node_modules/@web/dev-server/dist/bin.js" "$@"
fi
