#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/cpy-cli@4.2.0/node_modules/cpy-cli/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/cpy-cli@4.2.0/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/cpy-cli@4.2.0/node_modules/cpy-cli/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/cpy-cli@4.2.0/node_modules:/Users/clement-hecquet-evt/Documents/clement/node_test/c2pa-js/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
