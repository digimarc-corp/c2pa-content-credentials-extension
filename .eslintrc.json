{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "import/no-relative-packages": "off",
        "no-empty-pattern": ["error", { "allowObjectPatternsAsParameters": true }]
    }
}
